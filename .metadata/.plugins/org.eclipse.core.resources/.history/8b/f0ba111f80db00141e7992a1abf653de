package comparator;

public class UserRating extends Comparator<User>{
	    
	    public final static FrequencySorter instance = new FrequencySorter();
	    
	    @Override
	    public int compare(HuffmanNode o1, HuffmanNode o2){
	        return Integer.compare(o1.getFrequency(), o2.getFrequency());
	    }
	    
	}
}
